            <Insets bottom="10.0" />
         </StackPane.margin></Button>
      <Text id="errorMessage" strokeType="OUTSIDE" strokeWidth="0.0" text="Error Message" textAlignment="CENTER" textOrigin="CENTER" wrappingWidth="200.0">
