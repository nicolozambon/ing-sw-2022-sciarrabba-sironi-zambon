<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<StackPane id="stackPane" minHeight="200.0" minWidth="-Infinity" prefWidth="400.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.polimi.ingsw.client.view.gui.controller.ErrorController">
   <children>
      <Button mnemonicParsing="false" onMouseClicked="#okButtonOnClick" prefHeight="35.0" prefWidth="76.0" text="OK" StackPane.alignment="BOTTOM_CENTER">
